{
  "name": "@adobe/aio-cli-plugin-config",
  "description": "Config Plugin for the Adobe I/O CLI",
  "version": "2.0.0-rc1",
  "author": "Adobe Systems Inc.",
  "bugs": "https://github.com/adobe/aio-cli-plugin-config/issues",
  "dependencies": {
    "@adobe/aio-cli-config": "^1.0.0",
    "@oclif/command": "^1",
    "conf": "^4.0.1",
    "hjson": "^3.1.2",
    "js-yaml": "^3.13.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.21.2",
    "@oclif/plugin-help": "^2.1.4",
    "@oclif/test": "^1",
    "babel-runtime": "^6.26.0",
    "eslint": "^5.15.1",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.13.10",
    "eslint-plugin-jest": "^22.1.2",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^24.1.0",
    "jest-plugin-fs": "^2.9.0"
  },
  "engines": {
    "node": ">= 8.10.0"
  },
  "files": [
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/adobe/aio-cli-plugin-config",
  "keywords": [
    "oclif-plugin"
  ],
  "license": "Apache-2.0",
  "oclif": {
    "commands": "./src/commands",
    "bin": "./bin/run",
    "devPlugins": [
      "@oclif/plugin-help"
    ],
    "hooks": {
      "init": "./src/hooks.js"
    },
    "topics": {
      "config": {
        "description": "list, get, set, delete, and edit persistent configuration data"
      }
    }
  },
  "main": "src/index.js",
  "repository": "adobe/aio-cli-plugin-config",
  "scripts": {
    "eslint": "eslint .",
    "test": "yarn eslint && yarn run jest",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "postpack": "rm -f oclif.manifest.json",
    "version": "oclif-dev readme && git add README.md"
  }
}

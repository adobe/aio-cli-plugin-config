{
  "name": "@adobe/aio-cli-plugin-config",
  "description": "Config Plugin for the Adobe I/O CLI",
  "version": "1.0.5",
  "author": "Adobe Systems Inc.",
  "bugs": "https://github.com/adobe/aio-cli-plugin-config/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/errors": "^1.1.2",
    "conf": "^1.4.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/plugin-help": "^1",
    "@oclif/test": "^1",
    "eslint": "^4",
    "eslint-config-oclif": "^1",
    "jest": "^23.0.1",
    "jest-junit": "^5.0.0",
    "stdout-stderr": "^0.1.9"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/oclif.manifest.json",
    "/src",
    "/package-lock.json"
  ],
  "homepage": "https://github.com/adobe/aio-cli-plugin-config",
  "keywords": [
    "oclif-plugin"
  ],
  "license": "Apache-2.0",
  "oclif": {
    "commands": "./src/commands",
    "bin": "./bin/run",
    "devPlugins": [
      "@oclif/plugin-help"
    ],
    "topics": {
      "config": {
        "description": "get, set, delete, and clear persistent configuration data"
      }
    }
  },
  "main": "src/index.js",
  "repository": "adobe/aio-cli-plugin-config",
  "scripts": {
    "posttest": "eslint src",
    "test": "npm run unit-tests",
    "unit-tests": "jest --ci",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "postpack": "rm -f oclif.manifest.json",
    "version": "oclif-dev readme && git add README.md"
  },
  "jest": {
    "collectCoverage": true,
    "testPathIgnorePatterns": [
      "<rootDir>/tests/fixtures/"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/tests/fixtures/"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ],
    "testEnvironment": "node",
    "setupTestFrameworkScriptFile": "./jest.setup.js"
  }
}
